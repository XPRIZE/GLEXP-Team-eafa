<?xml version="1.0" encoding="utf-8"?> 

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

<item android:id="@android:id/background">
    <shape>
                  <stroke
            android:color="@color/progress_bar"
           
            android:width="0.25dp"

             />
        <corners android:radius="5dip" />
        <gradient
                android:startColor="@color/reg_white"
             
                android:endColor="@color/reg_white"
                android:angle="90"
        />
    </shape>
</item>

<item android:id="@android:id/secondaryProgress">
    <clip>
        <shape>
            <corners android:radius="10dip" />
            <solid android:color="@color/reg_white" />
        </shape>
    </clip>
</item>
<item
    android:id="@android:id/progress"
>
    
    <clip>
        <shape>
                  <stroke
            android:color="@color/progress_bar_second"
           
            android:width="0.25dp"

             />
            
            <corners
                android:radius="5dip" />
            
            <gradient
                                    android:startColor="@color/progress_bar_second"
                     android:centerColor="@color/progress_bar"
                    android:centerX="0.2"
                    android:endColor="@color/progress_bar"
                    android:angle="0"
                     />
        </shape>
    </clip>
</item>

</layer-list><!-- From: file:/C:/Users/abors/StudioProjects/Motoli%20All%20Subjects/app/src/main/res/drawable/rounded_progressbar.xml -->