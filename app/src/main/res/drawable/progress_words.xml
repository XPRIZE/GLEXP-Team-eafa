<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <stroke
                android:color="@color/reg_white"

                android:width="3.5dp"

                />
            <corners android:radius="20dip" />
            <gradient
                android:startColor="@color/launchpagbck"

                android:endColor="@color/launchpagbck"
                android:angle="90"
                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="20dip" />
                <solid android:color="@color/reg_white" />
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress"
        >

        <clip>
            <shape>
                <stroke
                    android:color="@color/reg_white"

                    android:width="3.5dp"

                    />

                <corners
                    android:radius="20dip" />

                <gradient
                    android:startColor="@color/progress_bar_words_second"
                    android:centerColor="@color/progress_bar_words"
                    android:centerX="0.2"
                    android:endColor="@color/progress_bar_words"
                    android:angle="0"
                    />
            </shape>
        </clip>
    </item>

</layer-list>