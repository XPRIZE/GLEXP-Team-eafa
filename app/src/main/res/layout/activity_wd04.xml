<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Activity_BaseFrame"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_space">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.23"
            layout="@layout/activity_top_area" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.3"/>

    </LinearLayout>


    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/activityMainPart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <Space
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight=".23"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.25">

            <Space
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight=".03"/>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="visible" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:adjustViewBounds="true"
                    android:baselineAlignBottom="false"
                    android:contentDescription="@string/none"
                    android:cropToPadding="false"
                    android:scaleType="fitCenter"
                    android:src="@drawable/frm_wd04"
                    android:focusableInTouchMode="true"/>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >

                    <Space
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"/>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:baselineAligned="false">

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".5"/>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/activityTextBox1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/wd04_textbox_margins"
                                android:src="@drawable/rounded_text_selected" />

                            <TextView
                                android:id="@+id/activityText1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/smallRandomString"
                                android:gravity="center_vertical|center_horizontal"
                                android:text="@string/smallRandomString"
                                android:textColor="@color/normalBlack"
                                android:textSize="@dimen/wdFirstFontSize" />



                            <ImageButton
                                android:id="@+id/image1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:adjustViewBounds="true"
                                android:background="@drawable/rounded_border_unchosen"
                                android:contentDescription="@string/smallRandomString"
                                android:scaleType="fitCenter"
                                android:src="@drawable/btn_saucer_1"
                                android:visibility="invisible" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/activityTextBox2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/wd04_textbox_margins"
                                android:src="@drawable/rounded_text_selected"
                                android:visibility="invisible"/>

                            <TextView
                                android:id="@+id/activityText2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical|center_horizontal"
                                android:text="@string/smallRandomString"
                                android:textColor="@color/normalBlack"
                                android:textSize="@dimen/wdFirstFontSize"
                                android:layout_centerInParent="true"
                                android:visibility="invisible"/>



                            <ImageButton
                                android:id="@+id/image2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:adjustViewBounds="true"
                                android:background="@drawable/rounded_border_unchosen"
                                android:contentDescription="@string/smallRandomString"
                                android:scaleType="fitCenter"
                                android:src="@drawable/btn_saucer_2"
                                android:visibility="visible" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/activityTextBox3"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/wd04_textbox_margins"
                                android:src="@drawable/rounded_text_selected" />

                            <TextView
                                android:id="@+id/activityText3"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_alignBottom="@+id/activityTextBox3"
                                android:layout_alignLeft="@+id/activityTextBox3"
                                android:layout_alignRight="@+id/activityTextBox3"
                                android:layout_alignTop="@+id/activityTextBox3"
                                android:layout_centerHorizontal="true"
                                android:gravity="center_vertical|center_horizontal"
                                android:text="@string/smallRandomString"
                                android:textColor="@color/normalBlack"
                                android:textSize="@dimen/wdFirstFontSize" />



                            <ImageButton
                                android:id="@+id/image3"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:adjustViewBounds="true"
                                android:background="@drawable/rounded_border_unchosen"
                                android:contentDescription="@string/smallRandomString"
                                android:scaleType="fitCenter"
                                android:src="@drawable/btn_saucer_1"
                                android:visibility="invisible"
                                android:layout_alignBottom="@+id/activityTextBox3"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"/>

                        </RelativeLayout>

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".5"/>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:baselineAligned="false">

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".5"/>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/activityTextBox4"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/wd04_textbox_margins"
                                android:src="@drawable/rounded_text_selected"
                                android:visibility="visible"/>

                            <TextView
                                android:id="@+id/activityText4"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:contentDescription="@string/smallRandomString"
                                android:gravity="center_vertical|center_horizontal"
                                android:text="@string/smallRandomString"
                                android:textColor="@color/normalBlack"
                                android:textSize="@dimen/wdFirstFontSize"
                                android:layout_centerInParent="true"/>



                            <ImageButton
                                android:id="@+id/image4"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:adjustViewBounds="true"
                                android:background="@drawable/rounded_border_unchosen"
                                android:contentDescription="@string/smallRandomString"
                                android:scaleType="fitCenter"
                                android:src="@drawable/btn_saucer_2"
                                android:visibility="invisible" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/activityTextBox5"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/wd04_textbox_margins"
                                android:src="@drawable/rounded_text_selected" />

                            <TextView
                                android:id="@+id/activityText5"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical|center_horizontal"
                                android:text="@string/smallRandomString"
                                android:textColor="@color/normalBlack"
                                android:textSize="@dimen/wdFirstFontSize"
                                android:layout_centerInParent="true"/>



                            <ImageButton
                                android:id="@+id/image5"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:adjustViewBounds="true"
                                android:background="@drawable/rounded_border_unchosen"
                                android:contentDescription="@string/smallRandomString"
                                android:scaleType="fitCenter"
                                android:src="@drawable/btn_saucer_1"
                                android:visibility="invisible" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/activityTextBox6"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/wd04_textbox_margins"
                                android:src="@drawable/rounded_text_selected" />

                            <TextView
                                android:id="@+id/activityText6"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical|center_horizontal"
                                android:text="@string/smallRandomString"
                                android:textColor="@color/normalBlack"
                                android:textSize="@dimen/wdFirstFontSize"
                                android:layout_centerInParent="true"/>



                            <ImageButton
                                android:id="@+id/image6"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:adjustViewBounds="true"
                                android:background="@drawable/rounded_border_unchosen"
                                android:contentDescription="@string/smallRandomString"
                                android:scaleType="fitCenter"
                                android:src="@drawable/btn_saucer_2"
                                android:visibility="invisible" />

                        </RelativeLayout>

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".5"/>

                    </LinearLayout>

                    <Space
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"/>

                </LinearLayout>

            </RelativeLayout>

            <Space
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight=".03"/>

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight=".05"/>

    </LinearLayout>

</FrameLayout>